    <script>
        // --- Search Functionality ---
        const siteContent = [
            { title: "New Economic Policy 2026", type: "News", desc: "Measures to boost local businesses.", url: "/news/economic-policy-2026" },
            { title: "Winter Vaccination Drive", type: "Health", desc: "Flu shot program locations.", url: "/news/health-update" },
            { title: "University Grants", type: "Education", desc: "National Scholarship Program.", url: "/news/education-grants" },
            { title: "National Insurance", type: "Service", desc: "Contributions and benefits.", url: "/services/national-insurance" },
            { title: "Pay Taxes", type: "Service", desc: "Income and business tax.", url: "/services/taxes" },
            { title: "Visas & Immigration", type: "Immigration", desc: "Residency and work permits.", url: "/immigration/visa" },
            { title: "Employment Services", type: "Service", desc: "Jobs and labor laws.", url: "/services/employment" },
            { title: "Passport Renewal", type: "Service", desc: "Travel document services.", url: "/travel" },
            { title: "Contact Government", type: "Contact", desc: "Get in touch.", url: "/contact" }
        ];

        const searchOverlay = document.getElementById('searchOverlay');
        const desktopSearchTrigger = document.getElementById('desktopSearchTrigger');
        const searchClose = document.getElementById('searchClose');
        const searchInput = document.getElementById('searchInput');
        const searchResults = document.getElementById('searchResults');
        
        const mobileSearchInput = document.getElementById('mobileSearchInput');
        const mobileSearchBtn = document.getElementById('mobileSearchBtn');

        function openSearch(initialQuery = '') {
            searchOverlay.classList.add('active');
            searchInput.value = initialQuery;
            searchInput.focus();
            if(initialQuery) performSearch(initialQuery);
        }

        function closeSearch() {
            searchOverlay.classList.remove('active');
            searchInput.value = '';
            searchResults.innerHTML = '';
        }

        function performSearch(query) {
            const lowerQuery = query.toLowerCase();
            const filtered = siteContent.filter(item => 
                item.title.toLowerCase().includes(lowerQuery) || 
                item.desc.toLowerCase().includes(lowerQuery)
            );

            searchResults.innerHTML = filtered.length > 0 ? filtered.map(item => `
                <a href="${item.url}" class="result-item">
                    <span class="result-tag">${item.type}</span>
                    <div class="result-title">${item.title}</div>
                    <div class="result-desc">${item.desc}</div>
                </a>
            `).join('') : '<div style="text-align:center; color:#666; padding:20px;">No results found.</div>';
            
            if(query === '') searchResults.innerHTML = '';
        }

        desktopSearchTrigger.addEventListener('click', () => openSearch());
        searchClose.addEventListener('click', closeSearch);
        searchInput.addEventListener('input', (e) => performSearch(e.target.value));
        document.addEventListener('keydown', (e) => {
            if (e.key === 'Escape') closeSearch();
        });

        mobileSearchBtn.addEventListener('click', () => {
            const query = mobileSearchInput.value;
            if(query) openSearch(query);
        });
        
        mobileSearchInput.addEventListener('keypress', (e) => {
            if (e.key === 'Enter') {
                e.preventDefault();
                openSearch(mobileSearchInput.value);
            }
        });

        // --- Hamburger Menu ---
        const hamburger = document.getElementById('hamburger');
        const navMenu = document.getElementById('nav-menu');
        const langBtn = document.querySelector('.lang-btn');
        const langMenu = document.querySelector('.lang-menu');

        hamburger.addEventListener('click', () => {
            navMenu.classList.toggle('active');
        });

        document.querySelectorAll('.nav-link').forEach(n => n.addEventListener('click', () => {
            navMenu.classList.remove('active');
        }));

        langBtn.addEventListener('click', () => {
            langMenu.classList.toggle('active');
            const expanded = langBtn.getAttribute('aria-expanded') === 'true' || false;
            langBtn.setAttribute('aria-expanded', !expanded);
        });

        document.addEventListener('click', (e) => {
            if (!langBtn.contains(e.target) && !langMenu.contains(e.target)) {
                langMenu.classList.remove('active');
                langBtn.setAttribute('aria-expanded', 'false');
            }
        });

        // --- Back to Top ---
        const backToTopBtn = document.getElementById("backToTop");

        window.addEventListener('scroll', () => {
            if (window.scrollY > 300) {
                backToTopBtn.classList.add("show");
            } else {
                backToTopBtn.classList.remove("show");
            }
        });

        backToTopBtn.addEventListener('click', () => {
            window.scrollTo({ top: 0, behavior: 'smooth' });
        });

        // --- Message Board with Cloudflare D1 ---
        const messageForm = document.getElementById('messageForm');
        const submitStatus = document.getElementById('submitStatus');
        const messagesList = document.getElementById('messagesList');

        // Cloudflare Worker endpoint (you'll need to replace this with your actual Worker URL)
        const API_ENDPOINT = '/api/messages';

        // Multi-step form functionality
        let currentStep = 1;
        const totalSteps = 4;

        // Get all step elements
        const steps = document.querySelectorAll('.form-step');
        const progressSteps = document.querySelectorAll('.progress-step');
        const progressLine = document.getElementById('progressLine');

        // Navigation buttons
        document.querySelectorAll('.next-step').forEach(btn => {
            btn.addEventListener('click', () => {
                if (validateCurrentStep()) {
                    goToStep(currentStep + 1);
                }
            });
        });

        document.querySelectorAll('.prev-step').forEach(btn => {
            btn.addEventListener('click', () => {
                goToStep(currentStep - 1);
            });
        });

        function validateCurrentStep() {
            const currentStepElement = document.querySelector(`.form-step[data-step="${currentStep}"]`);
            const inputs = currentStepElement.querySelectorAll('input, textarea');
            
            let isValid = true;
            inputs.forEach(input => {
                if (!input.checkValidity()) {
                    input.reportValidity();
                    isValid = false;
                }
            });
            
            return isValid;
        }

        function goToStep(step) {
            if (step < 1 || step > totalSteps) return;
            
            // Hide current step
            steps.forEach(s => s.classList.remove('active'));
            progressSteps.forEach(s => s.classList.remove('active'));
            
            // Show target step
            const targetStep = document.querySelector(`.form-step[data-step="${step}"]`);
            targetStep.classList.add('active');
            
            // Update progress
            progressSteps.forEach((s, index) => {
                if (index < step - 1) {
                    s.classList.add('completed');
                } else if (index === step - 1) {
                    s.classList.add('active');
                } else {
                    s.classList.remove('completed');
                    s.classList.remove('active');
                }
            });
            
            // Update progress line
            const lineWidth = ((step - 1) / (totalSteps - 1)) * 100;
            progressLine.style.width = lineWidth + '%';
            
            // Update review if on step 4
            if (step === 4) {
                document.getElementById('reviewName').textContent = document.getElementById('userName').value;
                document.getElementById('reviewEmail').textContent = document.getElementById('userEmail').value;
                document.getElementById('reviewMessage').textContent = document.getElementById('userMessage').value;
            }
            
            currentStep = step;
        }

        // Accordion functionality for provinces
        document.querySelectorAll('.province-header').forEach(header => {
            header.addEventListener('click', function() {
                const provinceBody = this.nextElementSibling;
                
                // Toggle active class
                this.classList.toggle('active');
                provinceBody.classList.toggle('active');
            });
        });

        // Load messages on page load
        loadMessages();

        // Handle form submission
        messageForm.addEventListener('submit', async (e) => {
            e.preventDefault();
            
            const formData = {
                name: document.getElementById('userName').value,
                email: document.getElementById('userEmail').value,
                message: document.getElementById('userMessage').value
            };

            try {
                const response = await fetch(API_ENDPOINT, {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json',
                    },
                    body: JSON.stringify(formData)
                });

                const result = await response.json();

                if (response.ok) {
                    submitStatus.className = 'submit-status success';
                    submitStatus.textContent = 'Your message has been submitted successfully! Thank you for your feedback.';
                    submitStatus.style.display = 'block';
                    
                    // Reset form and go back to step 1
                    messageForm.reset();
                    goToStep(1);
                    
                    loadMessages(); // Reload messages to show the new one
                } else {
                    throw new Error(result.error || 'Submission failed');
                }
            } catch (error) {
                submitStatus.className = 'submit-status error';
                submitStatus.textContent = 'There was an error submitting your message. Please try again later.';
                submitStatus.style.display = 'block';
                console.error('Error:', error);
            }

            // Hide status message after 5 seconds
            setTimeout(() => {
                submitStatus.style.display = 'none';
            }, 5000);
        });

        // Load messages from database
        async function loadMessages() {
            try {
                const response = await fetch(API_ENDPOINT);
                const data = await response.json();

                if (response.ok && data.messages && data.messages.length > 0) {
                    messagesList.innerHTML = data.messages.map(msg => `
                        <div class="message-item">
                            <div class="message-header">
                                <span class="message-author">${escapeHtml(msg.name)}</span>
                                <span class="message-date">${formatDate(msg.created_at)}</span>
                            </div>
                            <div class="message-content">${escapeHtml(msg.message)}</div>
                        </div>
                    `).join('');
                } else {
                    messagesList.innerHTML = '<div style="text-align:center; color: var(--text-secondary); padding:20px;">No messages yet. Be the first to share your thoughts!</div>';
                }
            } catch (error) {
                messagesList.innerHTML = '<div style="text-align:center; color: #721c24; padding:20px;">Unable to load messages at this time.</div>';
                console.error('Error loading messages:', error);
            }
        }

        // Helper function to escape HTML
        function escapeHtml(text) {
            const div = document.createElement('div');
            div.textContent = text;
            return div.innerHTML;
        }

        // Helper function to format date
        function formatDate(dateString) {
            const date = new Date(dateString);
            const options = { year: 'numeric', month: 'short', day: 'numeric', hour: '2-digit', minute: '2-digit' };
            return date.toLocaleDateString('en-US', options);
        }
    </script>
</body>
</html>
